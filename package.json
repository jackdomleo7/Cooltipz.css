  "bugs": {
    "url": "https://github.com/jackdomleo7/Cooltipz.css/issues"
  },
  "homepage": "https://cooltipz.jackdomleo.dev",
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "cssnano": "^4.1.11",
    "http-server": "^0.12.3",
    "node-sass": "^5.0.0",
    "postcss-cli": "^8.3.1",
    "sitemap-generator-cli": "^7.5.0",
    "stylelint": "^13.13.1",
    "stylelint-scss": "^3.19.0"
