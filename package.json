{
  "name": "cooltipz-css-docs",
  "version": "1.0.0",
  "private": true,
  "description": "Documentation website for Cooltipz.css",
  "scripts": {
    "docs": "npm run lint:fix && sass --no-source-map scss/docs.scss css/docs.min.css && sass --no-source-map scss/cooltipz.scss css/cooltipz.min.css && sass --no-source-map scss/prism.scss css/prism.min.css && sass --no-source-map scss/404.scss css/404.min.css && npx postcss css --use autoprefixer cssnano --replace",
    "serve": "http-server ./ --port 1234 -a localhost",
    "lint:check": "stylelint \"**/*.scss\"",
    "lint:fix": "stylelint \"**/*.scss\" --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackdomleo7/Cooltipz.css.git"
  },
  "author": {
    "name": "Jack Domleo",
    "url": "https://jackdomleo.dev"
  },
  "contributors": [
    "Jack Domleo <jackdomleo7@gmail.com> (https://jackdomleo.dev)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jackdomleo7/Cooltipz.css/issues"
  },
  "homepage": "https://cooltipz.jackdomleo.dev",
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "cssnano": "^5.0.10",
    "http-server": "^14.0.0",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.2",
    "sass": "^1.43.4",
    "sitemap-generator-cli": "^7.5.0",
    "stylelint": "^14.0.1",
    "stylelint-config-standard-scss": "^2.0.1"
  },
  "dependencies": {
    "cooltipz-css": "^2.1.0",
    "normalize.css": "^8.0.1"
  }
}
